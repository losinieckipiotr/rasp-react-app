{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access":true,
    "triple-equals": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-console": false,
    "no-submodule-imports": false,
    "no-empty-interface": false,
    "no-empty": false,
    "no-implicit-dependencies": false
  }
}